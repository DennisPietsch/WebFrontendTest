@page
@model RazorPagesMovie.Pages.Kunden.IndexModel
@using Microsoft.AspNetCore.Identity;
@using RazorPagesMovie.Models;
 
@{
    ViewData["Title"] = "Index";
}
<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Kunde[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kunde[0].Email)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.UserList)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
    </tr>
        }
    </tbody>
    <tbody>
    </tbody>
</table>
