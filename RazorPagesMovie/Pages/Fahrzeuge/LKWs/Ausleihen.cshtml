@page
@model RazorPagesMovie.Pages.Fahrzeuge.LKWs.AusleihenModel
@{
    ViewData["Title"] = "Ausleihen";
}

<h1>Test</h1>

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="LKW.Ausleihzeit" class="control-label"></label>
                <input asp-for="LKW.Ausleihzeit" class="form-control" />
