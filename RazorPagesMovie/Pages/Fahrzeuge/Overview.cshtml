@page
@model RazorPagesMovie.Pages.Fahrzeuge.OverviewModel
@{
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fahrzeugliste[0].ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fahrzeugliste[0].Hersteller)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fahrzeugliste[0].Preis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fahrzeugliste[0].Kundenname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fahrzeugliste[0].AusgeliehenBIS)
            </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.fahrzeugliste)
         {
            @if (item.Verfuegbar == false)
             {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Hersteller)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Preis)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Kundenname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AusgeliehenBIS)
                    </td>
                </tr>
             }
         }

    </tbody>
</table>
